@model GiayDep.Models.PhieuNhap

@{
    ViewData["Title"] = "Details";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

<h1>Chi tiết</h1>

<div>
    <h4>Phiếu nhập</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Ngày nhập
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Ngaynhap)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.IdnhaccNavigation)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdnhaccNavigation.Idnhacc)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Idphieunhap">Chỉnh sửa</a> |
    <a asp-action="Index">Trở về</a>
</div>
