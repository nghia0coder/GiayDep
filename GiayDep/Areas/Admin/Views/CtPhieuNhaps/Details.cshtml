@model GiayDep.Models.CtPhieuNhap

@{
    ViewData["Title"] = "Details";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

<h1>Chi tiết </h1>

<div>
    <h4>Chi tiết phiếu nhập </h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Ngày nhập 
        </dt>

        <dt class = "col-sm-2">
            Số lượng 
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Soluong)
        </dd>
        <dt class = "col-sm-2">
            Giá 
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Gia)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.IdphieunhapNavigation)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdphieunhapNavigation.Idphieunhap)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.IdspNavigation)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdspNavigation.Idsp)
        </dd>
    </dl>
</div>
<div>

    <a asp-action="Index">Trở về</a>
</div>
